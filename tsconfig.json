{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "experimentalDecorators": true,
    "jsx": "react",
    "keyofStringsOnly": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "build",
    "pretty": true,
    "strictNullChecks": false,
    "target": "es6",
    "types": ["react", "react-native"],
    "noEmit": true
  }
}